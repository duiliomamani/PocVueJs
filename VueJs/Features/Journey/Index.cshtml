
<div id="journey">
    <b-button v-on:click="journey" class="card-link">Crear Journey</b-button>
</div>

@section scripts{
    @if (Environment.GetEnvironmentVariable("ASPNETCORE_ENVIRONMENT") == "Development")
    {
        <script type="text/javascript" src="~/scripts/Journey/JourneyController.js" asp-append-version="true"></script>
     }

    <script>
        Common.Controller.initializeChild(new Vue(new VueJs.Journey.JourneyController));
    </script>
}
